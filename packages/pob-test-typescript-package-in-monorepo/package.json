{
  "name": "pob-test-typescript-package-in-monorepo",
  "version": "2.2.0",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./dist/index-node16.mjs",
  "types": "./dist/definitions/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./dist/index-node16.mjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.json",
    "clean": "yarn clean:build",
    "clean:build": "pob-babel-clean-out dist",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet packages/pob-test-typescript-package-in-monorepo",
    "test": "yarn ../../ run test -- packages/pob-test-typescript-package-in-monorepo",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "prettier": "@pob/root/prettier-config",
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "16"
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "pob-test-package-in-monorepo": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "pob-babel": "35.6.1",
    "typescript": "4.9.5"
  }
}
